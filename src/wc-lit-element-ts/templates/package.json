{
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./src/<%= className %>.js": {
      "default": "./dist/src/<%= className %>.js"
    },
    "./define/<%= tagName %>.js": {
      "default": "./dist/define/<%= tagName %>.js"
    },
    "./index.js": {
      "default": "./dist/index.js"
    },
    "./src/styles/<%= tagName %>-styles.css.js": {
      "default": "./dist/src/styles/<%= tagName %>-styles.css.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "/dist/",
    "/dist/index.*",
    "!/dist/test/"
  ],
  "scripts": {
    "build": "npm run build:tsc && npm run dev:vite",
    "build:tsc": "tsc",
    "sass:watch": "sass-style-template -j ts",
    "start:wds": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"npm:sass:watch\" \"npm:wds\"",
    "wds": "web-dev-server"
  },
  "devDependencies": {
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  }
}
