{
  "name": "<%= nameSpace %><%= tagName %>",
  "version": "1.0.0-rc.1",
  "description": "Webcomponent <%= tagName %> following open-wc recommendations",
  "keywords": [
    "lit",
    "web-component",
    "lit-element"
  ],
  "author": "<%= tagName %>",
  "license": "MIT",
  "customElements": "custom-elements.json",
  "type": "module",
  "scripts": {
    "start": "concurrently -k -r \"npm:vite\" \"npm:sass:watch\"",
    "vite": "vite",
    "dev:vite": "vite build",
    "preview:vite": "vite preview",
    "analyze": "cem analyze --litelement --globs \"src/**/*.{js,ts}\" \"index.js\"",
    "sort:package": "npx sort-package-json",
    "postinstall": "npm run sort:package",
    "prepare": "husky",
    "test": "vitest --run --browser.headless",
    "test:watch": "vitest"
  },
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@blockquote/cem-to-markdown-readme": "^1.0.0",
    "@blockquote/rollup-plugin-externalize-source-dependencies": "^1.0.0",
    "@blockquote/rollup-plugin-total-bundlesize": "^2.2.0",
    "@blockquote/sass-style-template": "^5.0.0",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@open-wc/semantic-dom-diff": "^0.20.1",
    "@open-wc/testing": "^4.0.0",
    "playwright": "^1.50.1",
    "postcss-html": "1.8.0",
    "postcss-lit": "^1.2.0",
    "@types/mocha": "^10.0.10",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "tinyglobby": "^0.2.12",
    "concurrently": "^9.1.2",
    "sort-package-json": "^2.14.0",
    "@vitest/browser": "^3.0.6",
    "@vitest/coverage-istanbul": "^3.0.6",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.6",
    "vitest": "^3.0.6",
    "vite": "^6.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
