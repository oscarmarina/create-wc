{
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./package.json": "./package.json",
    "./src/<%= className %>.js": {
      "default": "./src/<%= className %>.js"
    },
    "./define/<%= tagName %>.js": {
      "default": "./define/<%= tagName %>.js"
    },
    "./index.js": {
      "default": "./index.js"
    },
    "./src/styles/<%= tagName %>-styles.css.js": {
      "default": "./src/styles/<%= tagName %>-styles.css.js"
    }
  },
  "main": "index.js",
  "module": "index.js",
  "files": [
    "/define/",
    "/src/",
    "index.js",
    "!/**/*.scss"
  ],
  "scripts": {
    "build": "echo \"This is not a TypeScript project, so no need to build.\"",
    "sass:watch": "sass-style-template"
  }
}
