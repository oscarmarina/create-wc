{
  "exports": {
    ".": {
      "default": "./src/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./<%= className %>.js": {
      "default": "./src/<%= className %>.js"
    },
    "./<%= tagName %>.js": {
      "default": "./src/define/<%= tagName %>.js"
    },
    "./index.js": {
      "default": "./src/index.js"
    },
    "./<%= tagName %>-styles.css.js": {
      "default": "./src/styles/<%= tagName %>-styles.css.js"
    }
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "/src/",
    "!/**/*.scss"
  ],
  "scripts": {
    "build": "echo \"This is not a TypeScript project, so no need to build.\"",
    "sass:watch": "sass-style-template"
  }
}
